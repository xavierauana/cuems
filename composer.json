{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
	"framework",
	"laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "adldap2/adldap2-laravel": "^5.0",
    "barryvdh/laravel-cors": "^0.11.2",
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-dompdf": "^0.8.3",
    "dompdf/dompdf": "^0.8.3",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "5.7.*",
    "laravel/telescope": "^1.0",
    "laravel/tinker": "^1.0",
    "laravelcollective/html": "^5.7",
    "maatwebsite/excel": "^3.1",
    "simplesoftwareio/simple-qrcode": "^2.0",
    "spatie/laravel-activitylog": "^2.8",
    "spatie/laravel-medialibrary": "^7.5",
    "spatie/laravel-view-models": "^1.1",
    "stripe/stripe-php": "^6.17"
  },
  "require-dev": {
    "anacreation/utils-crud": "dev-develop",
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "guzzlehttp/guzzle": "^6.3",
    "laravel/dusk": "^4.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
	"files": [
	  "app/Helpers/setting.php",
	  "app/Helpers/country.php"
	],
	"classmap": [
	  "database/seeds",
	  "database/factories"
	],
	"psr-4": {
	  "App\\": "app/"
	}
  },
  "autoload-dev": {
	"psr-4": {
	  "Tests\\": "tests/"
	}
  },
  "extra": {
	"laravel": {
	  "dont-discover": [
	  ]
	}
  },
  "scripts": {
	"post-root-package-install": [
	  "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
	],
	"post-create-project-cmd": [
	  "@php artisan key:generate"
	],
	"post-autoload-dump": [
	  "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
	  "@php artisan package:discover"
	]
  },
  "config": {
	"preferred-install": "dist",
	"sort-packages": true,
	"optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
	{
	  "type": "vcs",
	  "url": "/Users/adrianexavier/code/packages/utils/crud"
	}
  ]
}
